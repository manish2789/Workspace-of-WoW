void neq ( ) {
	int  var  ;
		int  a ;
		int  b  ;
		var = (a != b)  ;
		

			
}void eq ( ) {
	int  var  ;
		int  a  ;
		int  b  ;
		var = (a == b)  ;
		
	
		
}void lt ( ) {
	int  var  ;
		int  a  ;
		int  b  ;
		var = (a < b)  ;
		
}void gt ( ) {
	int  var  ;
		int  a  ;
		int  b  ;
		var  =  (a > b)  ;
		
	
		
}void gteq ( ) {
	int  var  ;
		int  a  ;
		int  b  ;
		var =  (a >= b)  ;
		
	
		
}void lteq ( ) {
	int  var  ;
		int  a ;
		int  b  ;
		var  =  (a <= b)  ;
		
	
		
}void decrement ( ) {
	int  var  ;
		int  a  ;
		int  count  ;
		var = a - count ;
       
	
		
}void increment ( ) {
	int  var  ;
		int  a  ;
		int  count ;
		var = a + count ;
       
	
		
}void right_shift ( ) {
	int  shift_bits  ;
		int  a ;
		int  a_out  ;
		a_out = a >> shift_bits ;
       
		
}void left_shift ( ) {
	int  b_out  ;
		int  shift_bits  ;
		int  b  ;
		b_out = b << shift_bits ;
       
		
}
