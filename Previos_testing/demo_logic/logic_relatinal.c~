void xor ( ) {
	uint32_t  var = ;
		uint32_t  var = ;
		uint32_t  var = ;
		var = var ^ var ;
		
	var = XOR/output;
		
}void nand ( ) {
	uint32_t  var = ;
		uint32_t  var = ;
		uint32_t  var = ;
		var = ~ (var &amp; var) ;
		
	var = NAND/output;
		
}void nor ( ) {
	uint32_t  var = ;
		uint32_t  var = ;
		uint32_t  var = ;
		var = ~ (var | var) ;
		
	var = NOR/output;
		
}void nand ( ) {
	uint32_t  var = ;
		uint32_t  var = ;
		uint32_t  var = ;
		var = ~ (var &amp; var) ;
		
	var = NAND/output;
		
}void or ( ) {
	uint32_t  var = ;
		uint32_t  var = ;
		uint32_t  var = ;
		var = var || var ;
		
	var = OR/output;
		
}void and ( ) {
	uint32_t  var = ;
		uint32_t  var = ;
		uint32_t  var = ;
		var = (var ) &amp;(var)  ;
		
	var = AND_logic/output;
		
}